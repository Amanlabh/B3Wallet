{
  "name": "b3wallet-frontend",
  "version": "0.4.6",
  "author": "b3hr4d",
  "description": "Wallet and System Canister for the B3Wallet on the Internet Computer",
  "keywords": [
    "nextjs",
    "internet computer",
    "icp",
    "starter",
    "dfinity"
  ],
  "scripts": {
    "dev": "yarn env && source ../.env && DFX_NETWORK=local next dev",
    "ic:dev": "yarn ic:env && source ../.env && DFX_NETWORK=ic next dev",
    "env": "DFX_NETWORK=local node dfx.webpack.config.js init",
    "ic:env": "DFX_NETWORK=ic node dfx.webpack.config.js init",
    "build": "DFX_NETWORK=ic next build",
    "build:local": "DFX_NETWORK=local next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "lint:fix": "next lint --fix",
    "ic:deploy": "yarn ic:env && dfx deploy wallet_assets --network ic"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "^18.0.38",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3",
    "eslint-plugin-react": "^7.32",
    "eslint-plugin-react-hooks": "^4.6",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.6.1",
    "@dfinity/agent": "^0.15",
    "@dfinity/auth-client": "^0.15.6",
    "@dfinity/candid": "^0.15",
    "@dfinity/identity": "^0.15.6",
    "@dfinity/identity-secp256k1": "^0.15.6",
    "@dfinity/ledger": "^0.0.7",
    "@dfinity/nns": "^0.15.1",
    "@dfinity/principal": "^0.15",
    "@dfinity/utils": "^0.0.14",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "compressorjs": "^1.2.1",
    "ethers": "^5.7.2",
    "events": "^3.3.0",
    "framer-motion": "^10.12.16",
    "next": "^13.3",
    "node-fetch": "^2.6.6",
    "pem-file": "^1.0.1",
    "react": "^18.2",
    "react-dom": "^18.2",
    "react-dropzone": "^14.2",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  }
}

{
  "name": "nextjs_ic_template",
  "version": "0.4.0",
  "author": "Henry Chan",
  "description": "Next.js Internet Computer Starter Template",
  "keywords": [
    "nextjs",
    "internet computer",
    "icp",
    "starter",
    "dfinity"
  ],
  "scripts": {
    "dev": "next dev",
    "ic:dev": "DFX_NETWORK=ic next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "export": "next build",
    "ic:deploy": "dfx generate && dfx deploy b3_user --network ic",
    "env": "node dfx.webpack.config.js init",
    "generate": "cargo test && dfx generate"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "^18.0.38",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3",
    "eslint-plugin-react": "^7.32",
    "eslint-plugin-react-hooks": "^4.6",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@dfinity/agent": "^0.15",
    "@dfinity/auth-client": "^0.15.6",
    "@dfinity/candid": "^0.15",
    "@dfinity/identity": "^0.15.6",
    "@dfinity/identity-secp256k1": "^0.15.6",
    "@dfinity/principal": "^0.15",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "compressorjs": "^1.2.1",
    "ethers": "^5.7.2",
    "events": "^3.3.0",
    "next": "^13.3",
    "node-fetch": "^2.6.6",
    "pem-file": "^1.0.1",
    "react": "^18.2",
    "react-dom": "^18.2",
    "react-dropzone": "^14.2",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  }
}

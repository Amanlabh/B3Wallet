{
  "canisters": {
    "b3_user": {
      "candid": "backend/b3_user/b3_user.did"
    },
    "b3_wallet": {
      "type": "custom",
      "package": "b3_wallet",
      "build": "scripts/build-wallet.sh",
      "candid": "backend/b3_wallet/b3_wallet.did",
      "wasm": "wasm/b3_wallet/b3_wallet.wasm",
      "declarations": {
        "output": "src/declarations/b3_wallet",
        "node_compatibility": true
      }
    },
    "b3_system": {
      "type": "rust",
      "package": "b3_system",
      "candid": "backend/b3_system/b3_system.did",
      "dependencies": ["b3_wallet"],
      "declarations": {
        "output": "src/declarations/b3_system",
        "node_compatibility": true
      }
    },
    "wallet_assets": {
      "dependencies": ["b3_wallet"],
      "frontend": {
        "entrypoint": "out/index.html"
      },
      "source": ["out"],
      "type": "assets"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "wasm/identity/internet_identity.did",
      "wasm": "wasm/identity/internet_identity.wasm",
      "remote": {
        "candid": "internet_identity.did",
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "declarations": {
        "node_compatibility": true
      }
    },
    "ledger": {
      "type": "custom",
      "candid": "wasm/ledger/ledger.private.did",
      "wasm": "wasm/ledger/ledger.wasm",
      "remote": {
        "candid": "ledger.public.did",
        "id": {
          "ic": "ryjl3-tyaaa-aaaaa-aaaba-cai"
        }
      },
      "declarations": {
        "node_compatibility": true
      }
    },
    "cmc": {
      "type": "custom",
      "candid": "wasm/cmc/cmc.did",
      "wasm": "wasm/cmc/cmc.wasm",
      "remote": {
        "id": {
          "ic": "rkp4c-7iaaa-aaaaa-aaaca-cai"
        }
      },
      "declarations": {
        "node_compatibility": true
      }
    }
  },
  "dfx": "0.13.1",
  "defaults": {
    "replica": {
      "subnet_type": "system"
    }
  }
}
